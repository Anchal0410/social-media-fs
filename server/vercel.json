{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "JWT_SECRET": "@jwt_secret"
    }
  }